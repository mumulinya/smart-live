# 基础镜像
FROM  openjdk:8-jre
# author
MAINTAINER smartLive

# 挂载目录
VOLUME /home/smartLive
# 创建目录
RUN mkdir -p /home/smartLive
# 指定路径
WORKDIR /home/smartLive
# 复制jar文件到路径
COPY ./jar/smartLive-modules-follow.jar /home/smartLive/smartLive-modules-follow.jar
# 启动系统服务
ENTRYPOINT ["java","-jar","smartLive-modules-follow.jar"]